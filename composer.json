{
    "name": "php-etl/action",
    "description": "This library implements the Extract-Transform-Load pattern asynchronously in PHP with the help of iterators and generators",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kiboko SAS",
            "homepage": "http://kiboko.fr"
        },
        {
            "name": "Gr√©gory Planchat",
            "email": "gregory@kiboko.fr"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.2",
        "psr/log": "^3.0",
        "php-etl/action-contracts": "0.2.*",
        "php-etl/satellite-contracts": "0.1.*",
        "phpstan/phpstan": "^1.10"
    },
    "require-dev": {
        "rector/rector": "^0.15",
        "friendsofphp/php-cs-fixer": "^3.38"
    },
    "autoload": {
        "psr-4": {
            "Kiboko\\Component\\Action\\": "src/"
        }
    },
    "config": {
        "bin-dir": "bin",
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.2.x-dev"
        }
    }
